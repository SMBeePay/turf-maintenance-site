		<script>
			// Web Vitals monitoring
			function measureWebVitals() {
				if ('web-vital' in window) return;
				
				// Largest Contentful Paint (LCP)
				if ('PerformanceObserver' in window) {
					try {
						const lcpObserver = new PerformanceObserver((list) => {
							const entries = list.getEntries();
							const lastEntry = entries[entries.length - 1];
							console.log('LCP:', lastEntry.startTime);
						});
						lcpObserver.observe({ entryTypes: ['largest-contentful-paint'] });
					} catch (e) {}

					// First Input Delay (FID)
					try {
						const fidObserver = new PerformanceObserver((list) => {
							const entries = list.getEntries();
							entries.forEach(entry => {
								console.log('FID:', entry.processingStart - entry.startTime);
							});
						});
						fidObserver.observe({ entryTypes: ['first-input'] });
					} catch (e) {}

					// Cumulative Layout Shift (CLS)
					try {
						let clsValue = 0;
						const clsObserver = new PerformanceObserver((list) => {
							for (const entry of list.getEntries()) {
								if (!entry.hadRecentInput) {
									clsValue += entry.value;
								}
							}
							console.log('CLS:', clsValue);
						});
						clsObserver.observe({ entryTypes: ['layout-shift'] });
					} catch (e) {}
				}
			}

			// Reduce layout shift by setting dimensions early
			function preventLayoutShift() {
				const style = document.createElement('style');
				style.textContent = `
					.hero h1 { min-height: 72px; }
					.btn-primary, .btn-secondary { min-height: 54px; min-width: 140px; }
					.service-card, .benefit { min-height: 200px; }
				`;
				document.head.appendChild(style);
			}

			// Mobile performance optimizations
			function optimizeForMobile() {
				if (window.innerWidth <= 768) {
					// Reduce animations on mobile
					const style = document.createElement('style');
					style.textContent = `
						@media (max-width: 768px) {
							* { animation-duration: 0.1s !important; transition-duration: 0.1s !important; }
							.service-card:hover, .benefit:hover { transform: none !important; }
						}
					`;
					document.head.appendChild(style);

					// Enable passive event listeners
					document.addEventListener('touchstart', () => {}, { passive: true });
					document.addEventListener('touchmove', () => {}, { passive: true });
				}
			}

			// Preload next likely pages based on user behavior
			function smartPreloading() {
				if ('IntersectionObserver' in window) {
					const ctaObserver = new IntersectionObserver((entries) => {
						entries.forEach(entry => {
							if (entry.isIntersecting) {
								const links = ['/contact', '/services'];
								links.forEach(href => {
									const link = document.createElement('link');
									link.rel = 'prefetch';
									link.href = href;
									document.head.appendChild(link);
								});
								ctaObserver.unobserve(entry.target);
							}
						});
					}, { rootMargin: '50px' });
					
					const cta = document.querySelector('.cta');
					if (cta) ctaObserver.observe(cta);
				}
			}

			// Optimize loading of non-critical resources
			function optimizeResourceLoading() {
				if ('requestIdleCallback' in window) {
					requestIdleCallback(() => {
						// Preconnect to external domains during idle time
						const domains = ['https://fonts.googleapis.com', 'https://www.google-analytics.com'];
						domains.forEach(url => {
							const link = document.createElement('link');
							link.rel = 'preconnect';
							link.href = url;
							document.head.appendChild(link);
						});
					}, { timeout: 2000 });
				}
			}

			// Initialize all optimizations
			document.addEventListener('DOMContentLoaded', () => {
				preventLayoutShift();
				optimizeForMobile();
				smartPreloading();
				optimizeResourceLoading();
				measureWebVitals();
			});

